{
  "manifest_version": 2,

  "name": "Bookmark",
  "description": "Bookmark App",
  "version": "1.0",
  "content_scripts":[
		{
			"matches": ["http://assessment.resa.net/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
  "permissions": [
    "tabs", 
    "http://*/*", 
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}
